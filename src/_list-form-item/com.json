{
  "title": "获取表单项列表",
  "namespace": "mybricks.comlib-template-compute._list-form-item",
  "version": "1.0.1",
  "description": "输出所有可用的表单项信息列表",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.png",
  "data": "./data.json",
  "runtime": "./index.ts",
  "editors": "./editor.ts",
  "upgrade": "./upgrade.ts",
  "rtType": "js-autorun",
  "inputs": [
    {
      "id": "start",
      "title": "获取"
    }
  ],
  "outputs": [
    {
      "id": "formItemList",
      "title": "表单项信息列表",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "type": "string",
              "description": "表单项名称"
            },
            "value": {
              "type": "string",
              "description": "表单项唯一标识"
            }
          }
        }
      }
    }
  ]
}